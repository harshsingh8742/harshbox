=\"Overlay Color\" name=\"overlayColor\" value=\"#553fcc\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"circle-bg\" mbr-if=\"showCircle\"></div>\n            <div class=\"footer-top-container\" mbr-if=\"showLink || showTopButtons\">\n                <div class=\"col-link\">\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showLink\" data-app-selector=\".list\" data-multiline=\"true\" data-app-edit=\"full\" mbr-article>\n                        <li class=\"mbr-text item-wrap\" data-app-selector=\".list\">\n                            <a href=\"#\" class=\"text-white\" data-app-selector=\".list\">Home</a>\n                        </li>\n                        <li class=\"mbr-text item-wrap\" data-app-selector=\".list\">\n                            <a href=\"#\" class=\"text-white\" data-app-selector=\".list\">About</a>\n                        </li>\n                        <li class=\"mbr-text item-wrap\" data-app-selector=\".list\">\n                            <a href=\"#\" class=\"text-white\" data-app-selector=\".list\">Work</a>\n                        </li>\n                        <li class=\"mbr-text item-wrap\" data-app-selector=\".list\">\n                            <a href=\"#\" class=\"text-white\" data-app-selector=\".list\">Blog</a>\n                        </li>\n                        <li class=\"mbr-text item-wrap\" data-app-selector=\".list\">\n                            <a href=\"#\" class=\"text