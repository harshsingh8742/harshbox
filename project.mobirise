            "<script>\n  document.addEventListener(\"DOMContentLoaded\", function() {\n\n      // Disable Mobirise lightbox on images\n      document.body.addEventListener('click', function(e) {\n          const imgLink = e.target.closest('.mbr-gallery-item a');\n          if(imgLink && imgLink.querySelector('img')) {\n              e.preventDefault();\n              e.stopImmediatePropagation();\n              return false;\n          }\n      });\n\n      // Event delegation for gallery text links - now opens on same page\n      document.body.addEventListener('click', function(e) {\n          const textLink = e.target.closest('.mbr-gallery-text a');\n          if(textLink) {\n              e.stopPropagation(); // stop Mobirise handlers\n              return true; // allow default navigation on the same page\n          }\n      });\n\n      // Remove Mobirise modals/overlays and prevent page shrink\n      const removeOverlays = () => {\n          document.querySelectorAll('.modal-backdrop, .mbr-gallery-modal, .mbr-gallery .modal').forEach(el => el.remove());\n          document.body.classList.remove('modal-open');\n          document.body.style.overflow = '';\n          document.body.style.transform = 'none';\n          document.body.style.position = 'static';\n          document.body.style.width = 'auto';\n      };\n\n      removeOverlays();\n\n      // Observe DOM changes to continuously remove new overlays\n      const gallerySection = document.querySelector('section.gallery1');\nif (gallerySection) {\n  observer.observe(gallerySection, { attributes: true, childList: true, subtree: true });\n}\n\n\n  });\n  </script>"
